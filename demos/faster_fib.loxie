fun accumulate(a: int, b: int, it: int): int {
    if it < 1 {
        return a;
    } else {
        return accumulate(b, a + b, it - 1);
    }
}

fun fib(term: int): int {
    return accumulate(0, 1, term + 2);
}

fun main(): int {
    if fib(29) == 832040 {
        return 0;
    }

    return 1;
}