fun accumulate(a: int, b: int, it: int): int {
    if it < 1 {
        return b;
    } else {
        return accumulate(b, a + b, it - 1);
    }
}

fun fib(term: int): int {
    return accumulate(1, 1, term - 2);
}

fun main(): int {
    if fib(40) == 102334155 {
        return 0;
    }

    return 1;
}